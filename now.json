{
  "name": "she-is-carmen-api",
  "version": 2,
  "env": {
    "IS_NOW": "true"
  },
  "routes": [
    { "src": "/api/poem", "dest": "/src/backend/poem.ts", "methods": ["POST"] },
    { "src": "/", "dest": "/src/frontend/index.html" }
  ],
  "builds": [
    { "src": "**/*.ts", "use": "@now/node" },
    { "src": "**/*.html", "use": "@now/static" }
  ]
}
